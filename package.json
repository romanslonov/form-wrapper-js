{
  "name": "form-wrapper-js",
  "version": "0.8.0",
  "description": "JS abstraction for forms",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame src/**/*.ts tests/**/*.ts",
    "test": "jest",
    "test:watch": "npm run test --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.js",
    "watch": "rollup -c rollup.config.js -w",
    "codecov": "codecov",
    "prepublishOnly": "npm run build",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "npm run docs:build && gh-pages -m 'Auto deploy [ci skip]' -d docs/.vuepress/dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nevoss/form-wrapper-js"
  },
  "bugs": {
    "url": "https://github.com/Nevoss/form-wrapper-js/issues"
  },
  "homepage": "https://nevoss.github.io/form-wrapper-js",
  "keywords": [
    "forms",
    "form",
    "vue",
    "form-vue",
    "forms-vue",
    "js"
  ],
  "author": "Nevo Golan <nevos12@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "codecov": "^3.3.0",
    "gh-pages": "^2.0.1",
    "jest": "^24.7.1",
    "jest-extended": "^0.11.1",
    "prettier": "1.17.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.10.1",
    "rollup-plugin-typescript2": "^0.20.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.4",
    "vuepress": "^0.14.11"
  },
  "dependencies": {}
}
